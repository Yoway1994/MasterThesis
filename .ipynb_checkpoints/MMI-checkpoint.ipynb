    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "lambda_array = np.linspace(1350, 1550, 31)\n",
    "k_0 = 2*np.pi/(lambda_array)\n",
    "n_core = 1.82\n",
    "a = 10* (10**3) # nm\n",
    "m = 1\n",
    "n_m = n_core - ((4*m - 1)**2)*((np.pi)**2)/ (32* (k_0**2)* n_core* (a**2))\n",
    "\n",
    "plt.plot(lambda_array, n_m, 'bo')\n",
    "plt.show()"
   ]
  }
